<p id="notice"><%= notice %></p>

<h1>Camp Members</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone number</th>
      <th>Dob</th>
      <th>Age</th>
      <th>Email</th>
      <th>Paid</th>
      <th>Payment type</th>
      <th>Entered on</th>
      <th>Church</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @camp_members.each do |camp_member| %>
      <tr>
        <td><%= camp_member.first_name %></td>
        <td><%= camp_member.last_name %></td>
        <td><%= camp_member.street %></td>
        <td><%= camp_member.city %></td>
        <td><%= camp_member.state %></td>
        <td><%= camp_member.zip %></td>
        <td><%= camp_member.phone_number %></td>
        <td><%= camp_member.dob %></td>
        <td><%= camp_member.age %></td>
        <td><%= camp_member.email %></td>
        <td><%= camp_member.paid %></td>
        <td><%= camp_member.payment_type %></td>
        <td><%= camp_member.entered_on %></td>
        <td><%= camp_member.church %></td>
        <td><%= link_to 'Show', camp_member %></td>
        <td><%= link_to 'Edit', edit_camp_member_path(camp_member) %></td>
        <td><%= link_to 'Destroy', camp_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Camp Member', new_camp_member_path %>
